				},
				{
					"name": "Test Read Rate Limit (200 per min)",
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{baseUrl}}/employees?page=0&size=10",
							"host": [
								"{{baseUrl}}"
							],
							"path": [
								"employees"
							],
							"query": [
								{
									"key": "page",
									"value": "0"
								},
								{
									"key": "size",
									"value": "10"
								}
							]
						},
						"description": "Test read rate limit. Check X-RateLimit-Remaining header. Limit: 200 requests per minute."
					},
					"response": []
				},
				{
					"name": "Check Rate Limit Headers",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Rate limit headers present\", function () {",
									"    pm.response.to.have.header(\"X-RateLimit-Limit\");",
									"    pm.response.to.have.header(\"X-RateLimit-Remaining\");",
									"});",
									"",
									"var limit = pm.response.headers.get(\"X-RateLimit-Limit\");",
									"var remaining = pm.response.headers.get(\"X-RateLimit-Remaining\");",
									"",
									"console.log(\"Rate Limit:\", limit);",
									"console.log(\"Remaining Requests:\", remaining);"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{baseUrl}}/employees",
							"host": [
								"{{baseUrl}}"
							],
							"path": [
								"employees"
							]
						},
						"description": "Check rate limit headers in response"
					},
					"response": []
				}
			],
			"description": "Test rate limiting functionality"
		},
