								"exec": [
									"if (pm.response.code === 200) {",
									"    var jsonData = pm.response.json();",
									"    pm.collectionVariables.set(\"accessToken\", jsonData.data.accessToken);",
									"    pm.collectionVariables.set(\"refreshToken\", jsonData.data.refreshToken);",
									"    pm.collectionVariables.set(\"employeeId\", jsonData.data.id);",
									"    console.log(\"Access Token:\", jsonData.data.accessToken);",
									"    console.log(\"Refresh Token:\", jsonData.data.refreshToken);",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"auth": {
							"type": "noauth"
						},
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"email\": \"admin@company.com\",\n  \"password\": \"Admin@123\"\n}"
						},
